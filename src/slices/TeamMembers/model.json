{
  "id": "TeamMembers",
  "type": "SharedSlice",
  "name": "TeamMembers",
  "legacyPaths": {
    "team_page::body2::team_members": "default"
  },
  "variations": [
    {
      "id": "default",
      "name": "Default",
      "description": "Default",
      "imageUrl": "",
      "docURL": "",
      "version": "initial",
      "primary": {},
      "items": {
        "name": {
          "type": "Text",
          "config": {
            "label": "Name",
            "placeholder": "Team member's full name"
          }
        },
        "role": {
          "type": "Text",
          "config": {
            "label": "Role",
            "placeholder": "Team member's job title/position"
          }
        },
        "image": {
          "type": "Image",
          "config": {
            "constraint": {
              "width": 400,
              "height": 400
            },
            "thumbnails": [],
            "label": "Image"
          }
        },
        "bio": {
          "type": "StructuredText",
          "config": {
            "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl",
            "label": "Bio",
            "placeholder": "Short biography or introduction"
          }
        },
        "details": {
          "type": "StructuredText",
          "config": {
            "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl",
            "label": "Details",
            "placeholder": "Detailed achievements/background (separate each point with a new line)"
          }
        },
        "linkedin_url": {
          "type": "Link",
          "config": {
            "allowTargetBlank": true,
            "label": "LinkedIn URL",
            "placeholder": "LinkedIn profile URL (optional)",
            "select": null
          }
        },
        "twitter_url": {
          "type": "Link",
          "config": {
            "allowTargetBlank": true,
            "label": "Twitter URL",
            "placeholder": "Twitter profile URL (optional)",
            "select": null
          }
        },
        "github_url": {
          "type": "Link",
          "config": {
            "allowTargetBlank": true,
            "label": "GitHub URL",
            "placeholder": "GitHub profile URL (optional)",
            "select": null
          }
        }
      }
    }
  ]
}
